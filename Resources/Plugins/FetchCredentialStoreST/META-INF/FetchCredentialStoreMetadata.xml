<?xml version="1.0" encoding="UTF-8"?>
<scheduledTasks xmlns="http://xmlns.oracle.com/oim/scheduler">
    <task>
        <name>Fetch From Credential Store</name>
        <class>com.blogspot.oraclestack.scheduledtasks.FetchFromCredentialStore</class>
        <description>Get credential of service accounts through WebLogic Credential Store</description>
        <retry>5</retry>
        <parameters>
             <string-param required="true" encrypted="false" helpText="Map">Map</string-param>
             <string-param required="true" encrypted="false" helpText="Key">Key</string-param>
        </parameters>
    </task>
</scheduledTasks>